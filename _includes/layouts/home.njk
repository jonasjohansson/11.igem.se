---
layout: layouts/base.njk
section: home
---

<section>
    <p>{{ metadata.description }}</p>
    <nav>
        {%- for nav in collections.nav -%}
        <a href="{{ nav.url | url }}"{>{{ nav.data.theme.emoji }} {{ nav.data.title }}</a>
        {%- endfor -%}
    </nav>
    <span>Team</span>
    <nav>
        {% for member in metadata.members %}
        <div>
            <span>{{ member.emoji }} {{ member.name }}</span>
        </div>
        {% endfor %}
    </nav>
</section>